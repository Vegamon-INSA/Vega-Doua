public class Attaques {
	
	public String nom;
	public int type;
	public int ptsAtt;
	public int ptsSoin;
	public int ptsDef;
	
	public Attaques (String nom, int type, int ptsAtt, int ptsSoin, int ptsDef){
		this.nom=nom;
		this.type=type;
		this.ptsAtt=ptsAtt;
		this.ptsSoin=ptsSoin;
		this.ptsDef=ptsDef;
	}
}

